{
	"author": "Blocknet",
	"background": {
		"persistent": true,
		"scripts": [
			"js/jssha.js",
			"js/analytics.js",
			"js/buy.js",
			"js/functions.js",
			"js/delegate.js",
			"js/md5.js",
			"js/Alert.js",
		  	"js/Cache.js",
			"js/Command.js",
			"js/Exchange.js",
			"js/NumberObject.js",
			"js/Request.js",
			"js/Storage.js",
			"js/TradingView.js",
			"js/TradingViewListener.js",

			"js/_1Broker.js",
			"js/Binance.js",
			"js/Bitfinex.js",
			"js/BitMEX.js",
			"js/BitMEXTestnet.js",
			"js/Bittrex.js",
			"js/GDAX.js",
			"js/Kraken.js",
			"js/Kucoin.js",
			"js/OKCoin.js",
			"js/OKEX.js",
			"js/Poloniex.js",
			"js/PWP.js",

			"js/Broker.js",
			"background.js"
		]
	},
	"browser_action": {
		"default_icon": {
			"19": "img/icons/logo.png",
			"38": "img/icons/logo.png"
		},
		"default_popup": "popup.html",
		"default_title": "Optimus"
	},
	"content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
    "content_scripts": [ {
		"js": [
			"js/functions.js",
			"content.js"
		],
		"matches": [ "https://*.tradingview.com/*" ],
		"run_at": "document_start"
	} ],
	"description": "TradingView can now become automated trading.",
	"icons": {
		"16": "img/icons/logo.png",
		"48": "img/icons/logo.png",
		"128": "img/icons/logo.png"
	},
	
	"manifest_version": 2,
	"minimum_chrome_version": "45.0",
	"name": "Optimus",
	"optional_permissions": [
		"clipboardWrite",
		"webRequest",
		"webRequestBlocking",
		"https://*.1broker.com/*",
	  	"https://*.binance.com/*",
	  	"https://*.bitfinex.com/*",
		"https://*.bitmex.com/*",
		"https://*.bittrex.com/*",
	  	"https://*.gdax.com/*",
		"https://*.kraken.com/*",
		"https://*.kucoin.com/*",
		"https://*.okcoin.com/*",
		"https://*.okex.com/*",
		"https://*.poloniex.com/*"
	],
	"options_page": "options.html",
	"permissions": [
	  	"identity",
	  	"identity.email",
		"storage",
		"https://*.tradingview.com/*"
	],
	"short_name": "Optimus",
	"version": "1.0.1",
	"web_accessible_resources": [
		"js/analytics.js",
		"js/functions.js",
		"js/Autoview.js",
		"content_helper.js"
	]
}
